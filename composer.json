{
    "name": "ioffline/utils",
    "description": "Just some utils for my packages",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Kabin",
            "email": "me@ioffline.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.1|^8.2|^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IOF\\Utils\\": "src/"
        }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "0.x.x-dev"
      },
      "laravel": {
        "aliases": {
          "Sorter": "IOF\\Utils\\Sorter"
        }
      }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
